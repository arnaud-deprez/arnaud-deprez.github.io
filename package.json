{
  "name": "@goblindegook/gatsby-starter-typescript",
  "description": "A Gatsby starter the way I like it.",
  "version": "1.0.0",
  "author": "Lu√≠s Rodrigues <hello@goblindegook.com>",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "1.2.22",
    "@fortawesome/free-brands-svg-icons": "5.10.2",
    "@fortawesome/free-solid-svg-icons": "5.10.2",
    "@fortawesome/react-fontawesome": "0.1.4",
    "@mdx-js/mdx": "1.4.5",
    "@mdx-js/react": "1.4.5",
    "@pacote/react-use-outside": "1.1.0",
    "@reach/router": "1.2.1",
    "@types/lunr": "2.3.2",
    "bootstrap": "4.3.1",
    "core-js": "3.2.1",
    "gatsby": "2.15.16",
    "gatsby-plugin-catch-links": "2.1.9",
    "gatsby-plugin-feed": "2.3.12",
    "gatsby-plugin-lunr": "1.5.2",
    "gatsby-plugin-manifest": "2.2.16",
    "gatsby-plugin-mdx": "1.0.42",
    "gatsby-plugin-netlify": "2.1.13",
    "gatsby-plugin-nprogress": "2.1.6",
    "gatsby-plugin-offline": "3.0.7",
    "gatsby-plugin-react-helmet": "3.1.7",
    "gatsby-plugin-sass": "2.1.14",
    "gatsby-plugin-sharp": "2.2.22",
    "gatsby-plugin-sitemap": "2.2.12",
    "gatsby-plugin-typescript": "2.1.8",
    "gatsby-remark-autolink-headers": "2.1.13",
    "gatsby-remark-copy-linked-files": "2.1.19",
    "gatsby-remark-images": "3.1.22",
    "gatsby-remark-prismjs": "3.3.13",
    "gatsby-remark-smartypants": "2.1.8",
    "gatsby-source-filesystem": "2.1.22",
    "gatsby-transformer-remark": "2.6.22",
    "gatsby-transformer-sharp": "2.2.14",
    "lodash": "4.17.15",
    "lunr": "2.3.6",
    "node-sass": "4.12.0",
    "prismjs": "1.17.1",
    "ramda": "0.26.1",
    "react": "16.9.0",
    "react-bootstrap": "1.0.0-beta.12",
    "react-dom": "16.9.0",
    "react-helmet": "5.2.1",
    "react-icons": "3.7.0",
    "regenerator-runtime": "0.13.3"
  },
  "devDependencies": {
    "@babel/core": "7.6.0",
    "@babel/polyfill": "7.6.0",
    "@testing-library/jest-dom": "4.1.0",
    "@testing-library/react": "9.1.4",
    "@types/jest": "24.0.18",
    "@types/lodash": "4.14.138",
    "@types/node": "12.7.5",
    "@types/ramda": "0.26.22",
    "@types/react": "16.9.2",
    "@types/react-dom": "16.9.0",
    "@types/react-helmet": "5.0.10",
    "@typescript-eslint/eslint-plugin": "2.3.0",
    "@typescript-eslint/parser": "2.3.0",
    "eslint": "6.4.0",
    "eslint-config-prettier": "6.3.0",
    "eslint-config-standard": "14.1.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-node": "10.0.0",
    "eslint-plugin-prettier": "3.1.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-react": "7.14.3",
    "eslint-plugin-standard": "4.0.1",
    "husky": "3.0.5",
    "identity-obj-proxy": "3.0.0",
    "jest": "24.9.0",
    "lint-staged": "9.2.5",
    "prettier": "1.18.2",
    "ts-jest": "24.1.0",
    "typescript": "3.6.3",
    "webpack": "4.40.2"
  },
  "keywords": [
    "gatsby",
    "markdown",
    "typescript"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "start": "gatsby develop",
    "format": "prettier --write '{__mocks__,src,test}/**/*.{ts,tsx}' '*.js'",
    "prebuild": "npm run lint && npm run test",
    "lint": "eslint '{__mocks__,src,test}/**/*.{js,ts,tsx}'",
    "test": "jest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint",
      "git add"
    ],
    "*.{js,json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "jest": {
    "setupFilesAfterEnv": [
      "@testing-library/jest-dom/extend-expect",
      "./setup-jest.js"
    ],
    "globals": {
      "__PATH_PREFIX__": ""
    },
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(tsx?|jsx?)$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(gatsby)/)"
    ],
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx)$",
    "testPathIgnorePatterns": [
      "node_modules",
      ".cache"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "moduleNameMapper": {
      "typeface-*": "identity-obj-proxy",
      ".+\\.(css|styl|less|sass|scss)$": "identity-obj-proxy",
      ".+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/file.js"
    },
    "collectCoverage": false,
    "coverageReporters": [
      "lcov",
      "text",
      "html"
    ]
  }
}

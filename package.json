  "scripts": {
    "clean": "gatsby clean",
    "build": "npm run clean && gatsby build",
    "start": "npm run clean && gatsby develop -H 0.0.0.0",
    "format": "prettier --write '{__mocks__,src,test}/**/*.{ts,tsx}' '*.js'",
    "prebuild": "npm run lint && npm run test",
    "lint": "eslint '{__mocks__,src,test}/**/*.{js,ts,tsx}'",
    "test": "jest",
    "test:update": "jest -u",
    "debug:dev": "node --inspect node_modules/.bin/gatsby develop -H 0.0.0.0",
    "debug:prod": "node --inspect node_modules/.bin/gatsby build && gatsby serve -H 0.0.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint"
    ],
    "*.{js,json}": [
      "prettier --write"
    ]
  },
  "engines": {
    "node": "16"
  }
